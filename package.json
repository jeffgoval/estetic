{
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@hookform/resolvers": "^3.3.0",
    "@nhost/nhost-js": "^3.3.1",
    "@nhost/react": "^3.0.0",
    "graphql": "^16.8.0",
    "lucide-react": "^0.510.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.48.0",
    "react-router": "^7.5.3",
    "recharts": "^3.1.2",
    "zod": "^3.24.3",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "22.14.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "name": "mocha-app",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production npm run build",
    "dev": "vite",
    "lint": "eslint .",
    "nhost": "nhost dev",
    "nhost:prod": "nhost --config nhost.prod.toml",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:bash": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "preview": "vite preview",
    "docker:build": "docker build -t clinic-management .",
    "docker:run": "docker run -p 3000:80 clinic-management",
    "docker:dev": "docker-compose up -d"
  },
  "type": "module",
  "version": "0.0.0"
}
